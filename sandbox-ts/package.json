{
  "name": "rearchero-sandbox",
  "version": "0.0.1",
  "scripts": {
    "dev": "bun run --watch server.ts",
    "start": "bun run server.ts",
    "test": "bun test",
    "launch": "bun run scripts/launch.ts",
    "launch:device": "bun run scripts/launch.ts --device",
    "inject": "bun run scripts/launch.ts --no-server --attach",
    "inject:device": "bun run scripts/launch.ts --no-server --attach --restart-app --device",
    "inject:await": "bun run scripts/launch.ts --no-server --await",
    "inject:await:device": "bun run scripts/launch.ts --no-server --await --restart-app --device",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check ."
  },
  "dependencies": {
    "@typescript/native-preview": "^7.0.0-dev.20251228.1",
    "elysia": "^1.4.19",
    "hono": "^4.11.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/bun": "^1.3.5",
    "@types/node": "^25.0.3"
  }
}
