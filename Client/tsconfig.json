{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
"strict": false,
"noImplicitAny": false,
"noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "Android/**/*.ts",
    "iOS/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}