{
  "name": "rearchero",
  "version": "0.0.1",
  "description": "Frida instrumentation client for Archero",
  "private": true,
  "license": "MIT",
  "packageManager": "bun@1.3.5",
  "engines": {
    "bun": ">=1.3.0"
  },
  "scripts": {
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "build:ios": "frida-compile -o iOS/agent.js iOS/index.ts",
    "build:android": "frida-compile -o Android/agent.js Android/index.ts",
    "watch:ios": "frida-compile -o iOS/agent.js -w iOS/index.ts",
    "watch:android": "frida-compile -o Android/agent.js -w Android/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/bun": "^1.3.5",
    "@types/frida-gum": "^19.0.1",
    "@types/node": "^25.0.3",
    "frida-compile": "^19.0.4",
    "frida-il2cpp-bridge": "^0.12.1"
  }
}
