{
  "name": "rearchero",
  "version": "0.0.1",
  "description": "Frida instrumentation client for Archero",
  "private": true,
  "license": "MIT",
  "packageManager": "bun@1.3.5",
  "engines": {
    "bun": ">=1.3.0"
  },
  "scripts": {
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "build:ios": "frida-compile -o ios/agent.js ios/index.ts",
    "build:android": "frida-compile -o android/agent.js android/index.ts",
    "build:discovery": "frida-compile -o android/discovery.js android/discovery.ts",
    "watch:ios": "frida-compile -o ios/agent.js -w ios/index.ts",
    "watch:android": "frida-compile -o android/agent.js -w android/index.ts",
    "inject:android": "cd .. && /Library/Developer/CommandLineTools/usr/bin/python3 client/injector.py android",
    "inject:ios": "cd .. && /Library/Developer/CommandLineTools/usr/bin/python3 client/injector.py ios",
    "inject:macos": "cd .. && /Library/Developer/CommandLineTools/usr/bin/python3 client/injector.py macos",
    "discovery": "bun run build:discovery && frida -U -l android/discovery.js -f com.habby.archero"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/bun": "^1.3.5",
    "@types/frida-gum": "^19.0.1",
    "@types/node": "^25.0.3",
    "frida-compile": "^19.0.4",
    "frida-il2cpp-bridge": "^0.12.1"
  },
  "trustedDependencies": [
    "frida"
  ]
}
